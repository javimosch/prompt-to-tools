<div id="token-container" class="mb-4">
  <label for="token-input" class="block text-gray-700 text-sm font-bold mb-2">Token:</label>
  <input type="text" id="token-input" v-model="token" @input="setToken(token)" class="shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" />
</div>
